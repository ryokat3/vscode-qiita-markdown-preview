{
  "name": "vscode-qiita-markdown-preview",
  "displayName": "vscode-qiita-markdown-preview",
  "description": "VSCode extension to preview Qiita markdown",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-qiita-markdown-preview.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "unittest": "cross-env TS_NODE_PROJECT=tsconfig.json mocha --timeout 30000 --exit -r ts-node/register unittest/**/*.ts",
    "server": "http-server -d -c-1 -p 8080 html",
    "clean": "shx rm -rf out"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "http-server": "^14.1.0",
    "markdown-it": "^12.3.2",
    "mocha": "^9.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "markdown-it-container": "^3.0.0"
  }
}
